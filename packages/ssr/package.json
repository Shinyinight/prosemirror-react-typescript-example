{
  "name": "@example/client-ssr",
  "version": "0.1.0",
  "homepage": "https://teemukoivisto.github.io/prosemirror-react-typescript-example/",
  "scripts": {
    "start": "nodemon --watch server-dist server-dist/index.js",
    "lint": "eslint ./server/**/* ./client/**/*",
    "lint:fix": "eslint ./server/**/* --fix",
    "build": "npm run build:server && npm run build:client",
    "build:server": "rollup -c rollup.config.server.js",
    "build:client": "rollup -c rollup.config.client.js",
    "watch:server": "rollup -c rollup.config.server.js -w",
    "watch:client": "rollup -c rollup.config.client.js -w"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "22.0.1",
    "@rollup/plugin-json": "4.1.0",
    "@rollup/plugin-node-resolve": "^13.3.0",
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.13",
    "@types/lodash.debounce": "^4.0.7",
    "@types/morgan": "^1.9.3",
    "@types/node": "18.0.6",
    "@types/react": "18.0.15",
    "@types/react-dom": "18.0.6",
    "@types/react-icons": "3.0.0",
    "@types/react-router": "^5.1.18",
    "@types/react-router-dom": "^5.3.3",
    "@types/styled-components": "5.1.25",
    "@types/winston": "^2.4.4",
    "dotenv": "^16.0.1",
    "nodemon": "^2.0.19",
    "rollup": "^2.77.0",
    "rollup-plugin-inject-process-env": "1.3.1",
    "rollup-plugin-node-polyfills": "0.2.1",
    "rollup-plugin-postcss": "^4.0.2",
    "rollup-plugin-typescript2": "^0.32.1",
    "typescript": "^4.7.4"
  },
  "dependencies": {
    "@example/atlassian": "workspace:*",
    "@example/full": "workspace:*",
    "@example/minimal": "workspace:*",
    "cors": "^2.8.5",
    "express": "^4.18.1",
    "lodash.debounce": "^4.0.8",
    "morgan": "^1.10.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-icons": "4.4.0",
    "react-router": "^6.3.0",
    "react-router-dom": "^6.3.0",
    "styled-components": "5.3.5",
    "winston": "^3.8.1"
  }
}
